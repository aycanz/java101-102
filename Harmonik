public class Harmonik {
    public static void main(String[] args) {
        int[] numbers={1,2,3,4,5} ;
   float sum=0;
        for (int i = 0; i < numbers.length; i++) {
             sum+= 1.0 / numbers[i];


        }
        float harmonik = numbers.length/sum;

        System.out.println(harmonik);
    }
}
